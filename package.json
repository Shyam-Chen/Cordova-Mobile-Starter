{
  "name": "Cordova-Mobile-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Cordova, Material, Parcel, Babel, PostHTML, and PostCSS..",
  "scripts": {
    "start": "gulp --watch",
    "dev": "gulp --watch",
    "prod": "gulp --prod",
    "clean": "rimraf www",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "gulp": "gulp",
    "predev": "rimraf www",
    "preprod": "rimraf www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Cordova-Mobile-Starter.git"
  },
  "keywords": [
    "Cordova"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Cordova-Mobile-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Cordova-Mobile-Starter#readme",
  "dependencies": {
    "cordova-android": "^6.2.3",
    "cordova-plugin-firebase": "^0.1.21",
