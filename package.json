{
  "name": "Cordova-Phonegap-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Cordova, Material, Parcel, Babel, PostHTML, and PostCSS..",
  "scripts": {
    "start": "concurrently --kill-others \"parcel src/index.html -d www\"",
    "build": "parcel build src/index.html -d www",
    "cordova": "cordova"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Cordova-Phonegap-Starter.git"
  },
  "keywords": [
    "Cordova"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Cordova-Phonegap-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Cordova-Phonegap-Starter#readme",
  "dependencies": {
    "cordova-android": "6.4.0",
    "cordova-ios": "4.5.4",
    "cordova-plugin-whitelist": "1.3.3",
    "material-components-web": "0.32.0",
    "page": "1.8.4"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "babel-preset-env": "1.6.1",
    "concurrently": "3.5.1",
    "cordova": "8.0.0",
    "cssnano": "4.0.0-rc.2",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "htmlhint": "0.9.13",
    "parcel-bundler": "1.6.2",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "posthtml-include": "1.1.0",
    "rucksack-css": "1.0.2",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  }
}